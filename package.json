    "events": "^3.3.0",
    "iso-3166-1": "^2.1.1",
    "next": "^13.5.2",
    "react": "^18.2",
    "react-apexcharts": "^1.7.0",
    "react-dom": "^18.2",
    "react-dropzone": "^14.2",
    "react-icons": "^5.4.0",
    "react-minimal-pie-chart": "^9.1.0",
    "react-redux": "^9.2.0",
    "stream-browserify": "^3.0.0",
    "tailwind-merge": "^2.6.0",
    "util": "^0.12.5"
  }
